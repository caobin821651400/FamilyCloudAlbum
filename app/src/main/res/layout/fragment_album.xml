<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    android:clipToPadding="false">


    <com.family.cloud.album.view.IconTextButton
        android:id="@+id/btn_commemorate"
        style="@style/frg_album_icon_button_style"
        android:layout_width="128dp"
        android:layout_height="110dp"
        android:layout_margin="12dp"
        app:itb_text="纪念相册"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>


    <com.family.cloud.album.view.IconTextButton
        android:id="@+id/btn_Place"
        style="@style/frg_album_icon_button_style"
        android:layout_width="128dp"
        android:layout_height="110dp"
        android:layout_marginLeft="30dp"
        app:itb_text="地点"
        app:layout_constraintBottom_toBottomOf="@+id/btn_commemorate"
        app:layout_constraintLeft_toRightOf="@+id/btn_commemorate"
        app:layout_constraintTop_toTopOf="@+id/btn_commemorate" />

</androidx.constraintlayout.widget.ConstraintLayout>